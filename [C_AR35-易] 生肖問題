#include<bits/stdc++.h>    
using namespace std;    
int main()     
{  
    int a,which;  
    char data[13][10]= {"rat","ox","tiger","rabbit","dragon","snake","horse","sheep","monkey","rooster","dog","pig"};  
    cin>>a;  
    if(a<=2008 && (2008-a)%12==0)  
        which=0;  
    else if(a>2008)  
        which=(a-2008)%12;  
    else  
        which=12-(2008-a)%12;  
    cout<<data[which]<<endl;  
}
