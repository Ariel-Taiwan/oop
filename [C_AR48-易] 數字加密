#include<bits/stdc++.h>  
using namespace std;   
int main()   
{  
    int n[5],num;  
    cin>>num;  
    n[0]=num/1000;  
    n[1]=(num/100)%10;  
    n[2]=(num/10)%10;  
    n[3]=num%10;  
    for(int i=0;i<4;i++)  
        n[i]=(n[i]+7)%10;  
    cout<<n[2]<<n[3]<<n[0]<<n[1]<<endl;  
}
