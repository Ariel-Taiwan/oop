//https://e-tutor.itsa.org.tw/e-Tutor/mod/programming/view.php?id=3045
#include<bits/stdc++.h>  
using namespace std;  
int power(int a,int b){  
    if(b==0)  
        return 1;  
    else if(b==1)  
        return a;  
    else  
        return a*power(a,b-1);  
}  
int main()   
{  
    int a,b;  
    while(cin>>a>>b){  
        cout<<power(a,b)<<endl;  
    }  
}
