//https://e-tutor.itsa.org.tw/e-Tutor/mod/programming/view.php?id=3067
#include<bits/stdc++.h>  
using namespace std;  
int main()   
{  
    int flag=1,size=0;  
    char data[100],word;  
    while(cin>>word){  
        data[size++]=word;  
    }  
    for(int i=0;i<size/2;i++){  
        if(data[i]!=data[size-1-i]){  
            cout<<"NO"<<endl;  
            flag=0;  
            break;  
        }  
    }  
    if(flag==1)  
        cout<<"YES"<<endl;  
}
