#include<bits/stdc++.h>  
#include <array>  
using namespace std;  
int main()  
{  
    array<int,200>data;  
    char num;  
    data.fill(0);  
    while(cin>>num){  
        data[(int)num]++;  
    }  
    for(int i=127;i>=33;i--)  
        if(data[i])  
            cout<<i<<" "<<data[i]<<endl;  
}
